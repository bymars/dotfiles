	lg = log --graph --pretty=tformat:'%C(yellow)%h%Creset %C(bold green)%an, %ar%Creset - %s %C(cyan)%d%Creset'
	lgb = log --graph --pretty=tformat:'%C(yellow)%h%Creset %C(bold green)%an, %ar%Creset - %s %C(cyan)%d%Creset' --branches
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
